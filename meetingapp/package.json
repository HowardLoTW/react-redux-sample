{
  "name": "redux-practice",
  "version": "1.0.0",
  "description": "A practice for building react app using redux design pattern",
  "main": "server.js",
  "scripts": {
    "test": "BABEL_JEST_STAGE=0 jest",
    "build": "webpack --progress --colors --watch",
    "dev": "webpack-dev-server --devtool eval --progress --colors --content-base build",
    "deploy": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HowardLoTW/redux-practice.git"
  },
  "keywords": [
    "react",
    "redux",
    "jest",
    "todolist"
  ],
  "author": "Howard Lo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HowardLoTW/redux-practice/issues"
  },
  "homepage": "https://github.com/HowardLoTW/redux-practice#readme",
  "dependencies": {
    "classnames": "^2.2.1",
    "history": "^1.17.0",
    "parse": "^1.6.13",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.3",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-object-assign": "^1.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "jest-cli": "^0.8.2",
    "jsx-loader": "^0.13.2",
    "react-addons-test-utils": "^0.14.3",
    "redux-mock-store": "0.0.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "jest": {
    "setupEnvScriptFile": "<rootDir>/app/modules/jest_env_scripts/env.js",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/fbjs",
      "node_modules/parse"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js"
  }
}
